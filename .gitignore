/backend/*$py.class

# Distribution / packaging
/backend/dist/
/backend/build/
/backend/*.egg-info/
/backend/.eggs/

# Testing
/backend/.pytest_cache/
/backend/.coverage
/backend/htmlcov/

# Logs
/backend/*.log

# Local development
/backend/.idea/
/backend/.vscode/

# UV-specific
/backend/uv/
/backend/.uv/

